# FPD ETL Pipeline Environment Configuration
# Copy this file to .env and update with your values

# Google Cloud Storage Configuration
GCS_PROJECT_ID=your-gcp-project-id
GCS_BUCKET_NAME=your-gcs-bucket-name
# Optional: Path to service account key (if not using default auth)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Pipeline Configuration
ARTIFACTS_DIR=artifacts
LOG_LEVEL=INFO

# Optional: Pipeline defaults
DEFAULT_TOP_K_FEATURES=10
DEFAULT_MIN_SEQUENCE_LENGTH=10

# Development settings
# Set to "development" for verbose logging
ENVIRONMENT=production
